@{
    ViewBag.Title = "Trang chủ";
}

<input type="hidden" id="message" value="@TempData["Message"]" />

<!-- Banner -->
<section>
    <div class="container-lg">
        <div id="carouselExample" class="carousel slide" data-bs-ride="carousel">
            <div class="carousel-inner">
                <div class="carousel-item">
                    <img src="~/images/banner-1.png" class="d-block w-100" alt="...">
                </div>
                <div class="carousel-item active">
                    <img src="~/images/banner-2.png" class="d-block w-100" alt="...">
                </div>
            </div>
            <button class="carousel-control-prev" type="button" data-bs-target="#carouselExample" data-bs-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Previous</span>
            </button>
            <button class="carousel-control-next" type="button" data-bs-target="#carouselExample" data-bs-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Next</span>
            </button>
        </div>
    </div>
</section>

<!-- Best seller -->
<section class="pb-5">
    <div class="container-lg">

        <div class="row">
            <div class="col-md-12">

                <div class="section-header d-flex flex-wrap justify-content-between my-4">

                    <h2 class="section-title">Mặt hàng hiện có</h2>
                    <div class="d-flex align-items-center">
                        <a href="~/Product/Index" class="btn btn-primary rounded-1">Xem thêm</a>
                    </div>
                </div>

            </div>
        </div>

        <div class="row">
            <div class="col-md-12">
                <div class="product-grid row row-cols-1 row-cols-sm-2 row-cols-md-3 row-cols-lg-3 row-cols-xl-4 row-cols-xxl-5">
                    @{
                        var lstProduct = SV21T1020228.BusinessLayers.ProductDataService.ListOfProducts();
                        for (int i = 0; (i < 10 && i < lstProduct.Count); i++)
                        {
                            var item = lstProduct[i];
                            <div class="col">
                                <div class="product-item">
                                    <form action="~/Cart/Add/@item.ProductID" method="post">
                                        <figure>
                                            <a href="~/Product/Detail/@item.ProductID" title="Product Title">
                                                <img src="@WebConfig.IMAGE_SOURCE_HOST/images/products/@(string.IsNullOrWhiteSpace(item.Photo) ? "noProductAvailable.png" : @item.Photo)" alt="Product Thumbnail" class="tab-image overflow-hidden">
                                            </a>
                                        </figure>
                                        <div class="d-flex flex-column">
                                            <input type="hidden" name="@nameof(item.ProductID)" value="@item.ProductID" />
                                            <h3 class="fs-6 fw-normal text-wrap" style="height: 40px; overflow: hidden; display: -webkit-box; -webkit-box-orient: vertical; -webkit-line-clamp: 2;">@item.ProductName</h3>
                                            <div class="d-flex justify-content-center align-items-center gap-2">
                                                <span class="text-dark fw-semibold">@item.Price.ToString("#,0", new System.Globalization.CultureInfo("vi-VI")) VND</span>
                                            </div>
                                            <div class="button-area p-3 pt-0">
                                                <div class="row g-1 mt-2">
                                                    @* <div class="col-3">
                                            <input type="number" name="quantity" class="form-control border-dark-subtle input-number quantity" value="1">
                                            </div> *@
                                                    <div class="col">
                                                        <button type="submit" class="btn btn-primary rounded-1 p-2 fs-7 btn-cart">
                                                            <svg width="18" height="18"><use xlink:href="#cart"></use></svg>
                                                            Thêm vào Giỏ hàng
                                                        </button>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>

                        }
                    }
